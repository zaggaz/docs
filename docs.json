{
  "$schema": "https://mintlify.com/docs.json",
  "name": "Unibound",
  "theme": "mint",
  "colors": {
    "primary": "#6D0FAB",
    "light": "#9B30FF",
    "dark": "#6D0FAB"
  },
  "favicon": "/favicon.ico",
  "topbarCtaButton": {
    "name": "Book a Support Call",
    "url": "https://meetings.hubspot.com/james2781/support-call"
  },
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Application",
          "icon": "book-open-cover",
          "href": "https://app.unibound.ai"
        },
        {
          "anchor": "Website",
          "icon": "globe",
          "href": "https://unibound.ai"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "getting-started/overview",
              "getting-started/dashboard",
              "getting-started/navigation"
            ]
          },
          {
            "group": "Tutorials",
            "pages": [
              "getting-started/get-started",
              "tutorials/phone-agent",
              "tutorials/sms-agent",
              "tutorials/whatsapp-agent",
              "tutorials/email-agent",
              "tutorials/outreach-sequence"
            ]
          }
        ]
      },
      {
        "tab": "Pages",
        "groups": [
          {
            "group": "Agent Setup",
            "pages": [
              "agent-setup/agent-profile",
              "agent-setup/knowledge-base",
              "agent-setup/qualification",
              "agent-setup/booking",
              "agent-setup/review-prompt"
            ]
          },
          {
            "group": "Channels",
            "pages": [
              "channels/calls",
              "channels/sms",
              "channels/whatsapp",
              "channels/email",
              "channels/outreach-sequence"
            ]
          },
          {
            "group": "Configuration",
            "pages": [
              "configuration/phone-numbers",
              "configuration/email-accounts",
              "configuration/integrations",
              "configuration/notifications"
            ]
          },
          {
            "group": "Data",
            "pages": ["data/data-extraction"]
          },
          {
            "group": "Activity",
            "pages": [
              "activity/leads",
              "activity/conversations",
              "activity/conversation-details",
              "activity/escalations"
            ]
          },
          {
            "group": "Admin",
            "pages": ["admin/roles"]
          },
          {
            "group": "API Reference",
            "pages": [
              "api-reference/webhook-trigger",
              "api-reference/webhook-events",
              "api-reference/testing-webhooks"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/images/UniboundLogolight.svg",
    "dark": "/images/Unibounddark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@unibound.ai"
      }
