    "primary": {
      "type": "button",
      "label": "App",
      "href": "https://app.unibound.ai"
    }
  },
  "footer": {
    "socials": {
      "linkedin": "https://www.linkedin.com/company/uniboundagents"
    }
  },
  "fonts": {
    "family": "Plus Jakarta Sans"
  }
